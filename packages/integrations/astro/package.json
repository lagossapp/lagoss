{
  "name": "@lagoss/astro",
  "version": "0.4.1",
  "description": "Astro integration to deploy your site to Lagoss",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./server.js": "./dist/server.js",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lagossapp/lagoss"
  },
  "homepage": "https://github.com/lagossapp/lagoss/tree/main/packages/integrations/astro",
  "keywords": [
    "lagoss",
    "serverless",
    "edge",
    "integration",
    "astro",
    "withastro",
    "astro-adapter"
  ],
  "scripts": {
    "watch": "tsdown src/index.ts src/server.ts --format esm --dts --watch",
    "build": "tsdown src/index.ts src/server.ts --format esm --dts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "astro": "^5.17.3",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "esbuild": "^0.27.3"
  }
}
