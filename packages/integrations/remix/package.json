{
  "name": "@lagoss/remix",
  "version": "0.2.1",
  "description": "Remix integration to deploy your site to Lagoss",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lagossapp/lagoss"
  },
  "homepage": "https://github.com/lagossapp/lagoss/tree/main/packages/integrations/remix",
  "keywords": [
    "lagoss",
    "serverless",
    "edge",
    "integration",
    "remix"
  ],
  "scripts": {
    "watch": "tsdown src/index.ts --format esm --dts --watch",
    "build": "tsdown src/index.ts --format esm --dts",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@remix-run/server-runtime": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  }
}
